{
    "name": "sulu/test-bundle",
    "type": "sulu-bundle",
    "description": "Test Bundle for the Sulu CMF",
    "license": "MIT",
    "version": "dev-develop",
    "keywords": ["sulu", "test"],
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/sulu-cmf/SuluTestBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "sulu/sulu": "dev-develop",

        "phpspec/prophecy-phpunit": "^1.1",
        "symfony-cmf/testing": "^2.0",
    },
    "require-dev": {
        "symfony/symfony": "^2.8 || ^3.0",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/monolog-bundle": "^3.0",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-0": {
            "Sulu\\Bundle\\TestBundle": ""
        }
    },
    "target-dir": "Sulu/Bundle/TestBundle"
}
