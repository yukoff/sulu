{
    "name": "sulu/sulu",
    "type": "library",
    "description": "Sulu core distribution",
    "license": "MIT",
    "keywords": [
        "core",
        "sulu"
    ],
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/sulu-io/sulu/contributors"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:yukoff/sulu-document-manager.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:yukoff/massive-build-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:yukoff/massive-search-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:yukoff/zendframework-zendsearch.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "cboden/ratchet": "^0.4@dev",
        "doctrine/orm": "^2.5.3",
        "doctrine/annotations": "^1.2",
        "doctrine/phpcr-bundle": "^1.3.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.1",
        "friendsofsymfony/http-cache": "^2.0@beta",
        "jms/serializer-bundle": "^1.4",
        "friendsofsymfony/rest-bundle": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "imagine/imagine": "^0.6.1",
        "massive/search-bundle": "dev-develop",
        "sensio/framework-extra-bundle": "^3.0",
        "sulu/document-manager": "dev-develop",
        "stof/doctrine-extensions-bundle": "^1.2.2@dev",
        "gedmo/doctrine-extensions": "^2.4",
        "symfony-cmf/routing-bundle": "^1.2 || ^2.0",
        "symfony/symfony": "^2.8.7 || ^3.0",
        "willdurand/hateoas-bundle": "^1.2@dev",
        "symfony/swiftmailer-bundle": "^2.4",
        "pagerfanta/pagerfanta": "^1.0.4",
        "pulse00/ffmpeg-bundle": "^0.6",
        "oro/doctrine-extensions": "^1.1",
        "goodby/csv": "^1.3",
        "doctrine/data-fixtures": "^1.1",
        "layershifter/tld-extract": "^1.1",
        "mtdowling/cron-expression": "^1.1",
        "ramsey/uuid": "^3.1",
        "twig/twig": "^1.11 || ^2.0",
        "piwik/device-detector": "^3.7"
    },
    "require-dev": {
        "phpcr/phpcr-shell": "^1.0@dev",
        "jackalope/jackalope-doctrine-dbal": "^1.2.5",
        "massive/build-bundle": "^0.3.0",
        "php-http/guzzle6-adapter": "^1.0.0",

        "php-http/client-common": "^1.1.0",
        "php-http/message": "^1.0.0",
        "php-http/discovery": "^1.0",

        "matthiasnoback/symfony-config-test": "^1.1 || ^2.1",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.3 || ^1.1",
        "phpunit/phpunit": "^4.8.31",
        "phpspec/prophecy": "^1.4",
        "symfony/phpunit-bridge": "^2.8.7 || ^3.0",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/monolog-bundle": "^2.8.7 || ^3.0",
        "twig/twig": "~1.11",
        "zendframework/zend-stdlib": "^2.3 || ^3.1",
        "zendframework/zendsearch": "@dev",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "phpdocumentor/reflection-docblock": "^3.1"
    },
    "conflict": {
        "symfony/symfony": "2.8.10",
        "symfony/phpunit-bridge": "2.8.10",
        "symfony/monolog-bundle": "2.8.10",
        "jackalope/jackalope": "1.2.8 || 1.3.0 || 1.3.1"
    },
    "replace": {
        "sulu/media-bundle": "self.version",
        "sulu/tag-bundle": "self.version",
        "sulu/category-bundle": "self.version",
        "sulu/test-bundle": "self.version",
        "sulu/search-bundle": "self.version",
        "sulu/security-bundle": "self.version",
        "sulu/website-bundle": "self.version",
        "sulu/admin-bundle": "self.version",
        "sulu/generator-bundle": "self.version",
        "sulu/contact-bundle": "self.version",
        "sulu/snippet-bundle": "self.version",
        "sulu/location-bundle": "self.version",
        "sulu/translate-bundle": "self.version",
        "sulu/content-bundle": "self.version"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "^3.0",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/monolog-bundle": "^2.8 || ^3.0",
        "zendframework/zend-stdlib": "Needed (as in require dev) in addition to zendsearch",
        "zendframework/zendsearch": "To use the PHP based Zend Search library (based on Lucene)",
        "massive/build-bundle": "^0.3"
    },
    "autoload": {
        "psr-0": {
            "Sulu\\": "src/"
        },
        "exclude-from-classmap": [
            "src/Sulu/Component/*/Tests/",
            "src/Sulu/Bundle/*/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "Sulu\\": "tests/"
        }
    }
}
